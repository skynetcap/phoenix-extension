{
  "name": "Phoenix Access Extension",
  "version": "1.0",
  "description": "Helps access Phoenix when you are on international airplane WiFi.",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://www.phoenix.trade/*",
    "https://ipapi.co/json*"
  ],
  "background": {
    "scripts": [
      "phoenix.js"
    ],
    "persistent": true
  }
}